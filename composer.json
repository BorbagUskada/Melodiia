{
    "name": "biig/melodiia",
    "description": "Library that helps you build APIs",
    "type": "library",
    "scripts": {
        "phpcs": "php-cs-fixer fix --using-cache=false",
        "post-install-cmd": "make hooks.install"
    },
    "require": {
        "php": ">=7.2",
        "zircote/swagger-php": "^3.0.3",
        "nekland/tools": "^2.5.1",
        "zendframework/zend-json": "^3.0",
        "symfony/event-dispatcher": "^4.3 || ^5.0",
        "pagerfanta/pagerfanta": "^2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpspec/prophecy": "^1.8",
        "twig/twig": "^2.5",
        "symfony/symfony": "^4.3 || ^5.0",
        "doctrine/orm": "^2.6",
        "justinrainbow/json-schema": "^5.2",
        "doctrine/annotations": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Biig\\Melodiia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Biig\\Melodiia\\Test\\": "tests/Melodiia/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Maxime Veber",
            "email": "nek.dev@gmail.com"
        },
        {
            "name": "BiiG",
            "homepage": "https://www.biig.fr"
        }
    ]
}
