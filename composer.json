{
    "name": "biig/happii",
    "description": "Library that helps you build APIs with Symfony",
    "type": "library",
    "require": {
        "zircote/swagger-php": "^3.0",
        "nekland/tools": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13",
        "phpspec/prophecy": "^1.8",
        "twig/twig": "^2.5",
        "symfony/symfony": "^4.2"
    },
    "autoload": {
        "psr-4": {
